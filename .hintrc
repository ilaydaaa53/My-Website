{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": "off",
    "button-type": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "overflow-wrap: anywhere"
        ]
      }
    ],
    "compat-api/html": [
      "default",
      {
